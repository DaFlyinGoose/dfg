{
    "name": "fbf/laravel-blog",
    "description": "A Laravel 5 package to add a simple blog to a site",
    "keywords": ["laravel","blog"],
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        },
        {
            "name": "Chris Reid",
            "email": "chris.reid@nexuspoint.co.uk"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.*",
        "mayconbordin/rss-l5": "~1.1",
        "cviebrock/eloquent-sluggable": "4.*",
        "fzaninotto/faker": "1.4.*"
    },
    "autoload": {
        "classmap": [
